<launch>
    <node name="flip_pc_node" pkg="hanwha_sot" type="queue_pc.py" />
    <node name="queue_tf" pkg="hanwha_sot" type="queue_tf.py" />
    
    <node pkg="tf" type="static_transform_publisher" name="generate_os_updown" args="0 0 0 3.14159 0 0 os_sensor os_updown 200" />
    
    <node name="sot_node" pkg="hanwha_sot" type="hanwha_refactoring_code_v2.py" output="screen" >
        <param name="pt" value="$(find hanwha_sot)/scripts/pt/livox_model_2.pt"/>
    </node>
</launch>